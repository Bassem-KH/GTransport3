    {%for vehicule in vehicules%}
        <tr>
            <td>{{ vehicule.Id }}</td>
            <td>{{ vehicule.Marque }}</td>
            <td>{{ vehicule.NbrPlace }}</td>
            <td>{{ vehicule.NumImmatricule }}</td>
            <td>{{ vehicule.Kilomtetrage }}</td>
            <td>{{ vehicule.Modele }}</td>
            <td>{{ vehicule.DateDachat|date('d/m/y')}}</td>
            <td>{{ vehicule.Carburant }}</td>
            <td>{{ vehicule.EtatVehicule }}</td>
            <td>{{ vehicule.PuissanceFiscale }}</td>
            <td>{{ vehicule.PuissanceMecanique}}</td>
            <td>{{ vehicule.Disponibilite}}</td>
            <td>{{ vehicule.parc}}</td>
            <td>{{ vehicule.NomDuTransporteur}}</td>
            <td>{{ vehicule.TypeRemorque}}</td>
            <td> <a href="{{path("PPE_supprimer_VehiculeTransport",{'id':vehicule.Id})}}">suppimer</a></td>
            <td> <a href="{{path("PPE_modifier_VehiculeTransport",{'id':vehicule.Id})}}">modifier</a></td>
            
        </tr>
    {% endfor%}
    
    
</table>
